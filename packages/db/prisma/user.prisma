// O "engineType" que antigamente ficava aqui, agora Ã© gerenciado pelo prisma.config.ts!
generator client {
  provider = "prisma-client-js"
  output   = "../src/user/client"
}

generator zod {
  provider = "prisma-zod-generator"
  output   = "../../../packages/model/src/user"
}

datasource authDb {
  provider = "postgresql"
}

model User {
  id    String @id @default(uuid())
  email String @unique
}
